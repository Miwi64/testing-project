@page "/"

<PageTitle>Home</PageTitle>

<h1>Customers</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Edad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Customers)
        {
            <tr>
                <td>@customer.ID</td>
                <td>@customer.Name</td>
                <td>@customer.Edad</td>
            </tr>
        }
    </tbody>
</table>